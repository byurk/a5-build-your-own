# Copy to .env and fill in values. NEVER COMMIT .env.

# Gemini API key (Google AI Studio)
GEMINI_API_KEY=

# Model name
GEMINI_MODEL=gemini-2.5-flash

# Generation settings
GEMINI_TEMPERATURE=0.7

# Thinking/reasoning configuration (optional - defaults disable thinking)
# For Gemini 2.5 models: token budget (0 = disabled, 1024 = low, 8192 = medium, 16384 = high)
GEMINI_THINKING_BUDGET=0
# For Gemini 3+ models: reasoning depth (none, low, medium, high)
GEMINI_THINKING_LEVEL=none

# System prompt file (path to a .md file with system instructions)
# For A5: Use prompts/application_prompt.md (your custom prompt)
# Use prompts/empty.md for baseline testing with no system instructions
SYSTEM_PROMPT_FILE=prompts/application_prompt.md

# If 1, the app will attempt to use Gemini. If 0, uses a deterministic mock.
USE_GEMINI=0

# Document retrieval settings (for RAG)
RESOURCES_DIR=resources
CHUNK_SIZE=1000
CHUNK_OVERLAP=100
